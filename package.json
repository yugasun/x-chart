{
  "name": "x-chart",
  "version": "0.2.0",
  "description": "Draggable & resizable data visualization dashboard",
  "author": "yugasun <yuga_sun@163.com>",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:git-page": "cross-env GIT_PAGE=true vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run lint",
    "deploy": "npm run build:git-page && gh-pages -d dist --remote origin",
    "docker:build": "docker build -t x-chart .",
    "docker:run": "docker run -itd -p 8080:80 --name=x-chart x-chart",
    "docker:exec": "docker exec -it x-chart bash"
  },
  "dependencies": {
    "@types/echarts": "^4.1.3",
    "@types/mockjs": "^1.0.2",
    "@types/qs": "^6.5.1",
    "axios": "^0.18.0",
    "element-ui": "^2.4.11",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.0",
    "qs": "^6.5.2",
    "vue": "^2.5.17",
    "vue-axios-plugin": "^1.3.0",
    "vue-class-component": "^6.3.2",
    "vue-echarts": "^3.1.3",
    "vue-grid-layout": "git+https://github.com/yugasun/vue-grid-layout.git#pro/compass",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yugasun/x-chart.git"
  },
  "keywords": [
    "data-visualization",
    "echarts",
    "vuejs",
    "vue-grid-layout"
